<%= fields_for :address do |a| %>
  <%= a.text_field :address1, class: "form-control", placeholder: "Address 1" %></br>
  <%= a.text_field :address2, class: "form-control", placeholder: "Address 2" %></br>
  <%= a.text_field :city, class: "form-control", placeholder: "City" %></br>
  <%= a.text_field :state, class: "form-control", placeholder: "State" %></br>
  <%= a.text_field :zip_code, class: "form-control", placeholder: "Zip Code" %></br>
  <%= a.text_field :phone, class: "form-control", placeholder: "Phone Number" %></br>
<% end %>